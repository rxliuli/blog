{
  "name": "blog",
  "hexo": {
    "version": "5.4.0"
  },
  "scripts": {
    "clean": "rimraf .deploy_git && hexo clean",
    "build": "hexo g",
    "deploy": "git pull origin master && pnpm gen && esno script/commit.ts && hexo g -d",
    "start": "pnpm run clean && pnpm run deploy",
    "server": "hexo server",
    "new": "hexo new",
    "gen": "joplin-blog blog",
    "commit": "node scripts/commit.js"
  },
  "prettier": "@liuli-util/prettier-standard-config",
  "devDependencies": {
    "@liuli-util/prettier-standard-config": "^0.1.0",
    "@types/luxon": "^2.0.5",
    "babel-preset-es2015": "^6.24.1",
    "esno": "^0.10.1",
    "hexo": "^5.4.0",
    "hexo-deployer-git": "^3.0.0",
    "hexo-filter-github-emojis": "^3.0.4",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-sitemap": "^2.1.0",
    "hexo-generator-tag": "^1.0.0",
    "hexo-offline": "^1.0.0",
    "hexo-renderer-ejs": "^1.0.0",
    "hexo-renderer-marked": "^4.1.0",
    "hexo-renderer-stylus": "^2.0.1",
    "hexo-server": "^2.0.0",
    "hexo-theme-fluid": "^1.8.12",
    "joplin-blog": "^0.7.0",
    "luxon": "^2.0.2",
    "rimraf": "^3.0.2",
    "simple-git": "^2.47.0",
    "typescript": "^4.4.4"
  }
}
